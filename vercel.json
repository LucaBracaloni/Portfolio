{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.lucabracaloni.it/$1",
      "permanent": true
    }
  ]
} 